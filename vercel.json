{
  "version": 2,
  "buildCommand": "npm --prefix user-guard-verse-main run build",
  "outputDirectory": "user-guard-verse-main/dist",
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
